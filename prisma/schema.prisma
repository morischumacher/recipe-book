generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Recipe {
  id          Int      @id @default(autoincrement())
  title       String
  category    String
  description String
  ingredients String
  steps       String
  createdAt   DateTime @default(now())
}
