generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Recipe {
  id          Int      @id @default(autoincrement())
  title       String
  category    String
  description String
  ingredients String
  steps       String
  createdAt   DateTime @default(now())
}
